<script>
  const interval = setInterval(() => {
    const downloadLink = document.getElementById("download_sample");
    if (downloadLink) {
      // Replace with the template link 
      downloadLink.href = "https://docs.google.com/spreadsheets/d/1ITnT96wB0X5b3APJ18AS1zgU9gDRP6c6/edit?usp=sharing&ouid=102643583237523287893&rtpof=true&sd=true";
      downloadLink.removeAttribute("download"); 
      downloadLink.textContent = "View Spreadsheet Template";
      clearInterval(interval); 
    }
  }, 100); 
  const interval2 = setInterval(() => {
    //
    var elementToHide = document.querySelector('.import_specifics');
    if (elementToHide) {
     elementToHide.style.display = 'none'; 
     clearInterval(interval2); 
    }Â 
  }, 100); 
</script>
